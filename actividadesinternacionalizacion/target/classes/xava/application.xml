<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE application SYSTEM "dtds/application.dtd">

<application name="actividadesinternacionalizacion"> 
	
	<!-- 
	A default module for each entity is assumed with the
	controllers on <default-module/>
	-->
	<default-module>		  
		<controller name="Typical"/>		
	</default-module>
	
	<!-- 
	You can define additional modules, for example:	  
	<module name="OnlyReadCustomer">
		<model name="Customer"/>
		<controller name="Print"/>
	</module>		
	-->
	<module name="StatisticsDashboard">
		<model name="StatisticsDashboard"/>
		<controller name="InitModelWithNewInstance"/>
	</module>
</application>
